<?xml version="1.0" encoding="utf-8"?>

<xs:schema
	targetNamespace="http://h-cms.net/default.xsd"
	elementFormDefault="qualified"
	xmlns="http://h-cms.net/default.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsc="http://h-cms.net/custom">

	
	<!-- raw text -->

	<xs:element name="text" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="en">Plain text</xs:documentation>
			<xs:documentation xml:lang="fr">Texte brut</xs:documentation>
			<xs:appinfo>
				<xsc:properties textformat="text"></xsc:properties>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>

</xs:schema>
